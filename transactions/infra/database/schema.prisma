datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator db {
  provider = "go run github.com/prisma/prisma-client-go"
  output   = "db"
  package  = "db"
}

model Transaction {
  id          String   @id @unique
  seller_id   String
  buyer_id    String
  date        DateTime @default(now())
  price       Float
}
